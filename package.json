        "connect-mongo": "^4.4.1",
        "dotenv": "^8.2.0",
        "express-session": "^1.17.1",
        "if-env": "^1.0.4",
        "jsonwebtoken": "^8.5.1",
        "mongo": "^0.1.0",
        "mongoose": "^5.12.5",
        "nodemon": "^2.0.7",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "random-token": "0.0.8",
        "react-icons": "^4.2.0",
        "react-router-dom": "^5.2.0"
